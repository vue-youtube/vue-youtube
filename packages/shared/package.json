{
    "name": "@vue-youtube/shared",
    "version": "1.0.6",
    "description": "Shared code for VueYoutube",
    "author": "Techassi <git@techassi.dev>",
    "keywords": [
        "vue",
        "youtube",
        "iframe",
        "embed",
        "api",
        "youtube iframe api",
        "vue-youtube-iframe"
    ],
    "license": "MIT",
    "scripts": {
        "build": "vue-tsc --noEmit && vite build",
        "lint": "eslint --ext .js,.vue,.ts ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vue-youtube/vue-youtube.git",
        "directory": "packages/component"
    },
    "exports": {
        ".": {
            "import": "./index.mjs",
            "require": "./index.cjs",
            "types": "./index.d.ts"
        }
    },
    "main": "./index.cjs",
    "types": "./index.d.ts",
    "module": "./index.mjs",
    "sideEffects": false,
    "bugs": {
        "url": "https://github.com/vue-youtube/vue-youtube/issues"
    },
    "homepage": "https://github.com/vue-youtube/vue-youtube#readme",
    "peerDependencies": {
        "@vue/composition-api": "^1.1.0",
        "vue": "^2.6.0 || ^3.2.0"
    },
    "peerDependenciesMeta": {
        "vue": {
            "optional": true
        },
        "@vue/composition-api": {
            "optional": true
        }
    },
    "dependencies": {
        "vue-demi": "*"
    }
}