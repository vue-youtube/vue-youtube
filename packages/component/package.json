{
    "name": "@vue-youtube/component",
    "version": "1.1.0",
    "description": "Component for VueYoutube",
    "author": "Techassi <git@techassi.dev>",
    "keywords": [
        "vue",
        "youtube",
        "iframe",
        "embed",
        "api",
        "youtube iframe api",
        "vue-youtube-iframe"
    ],
    "license": "MIT",
    "scripts": {
        "build": "vue-tsc --noEmit && vite build",
        "lint": "eslint --ext .js,.vue,.ts ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vue-youtube/vue-youtube.git",
        "directory": "packages/component"
    },
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.mjs",
    "sideEffects": false,
    "bugs": {
        "url": "https://github.com/vue-youtube/vue-youtube/issues"
    },
    "homepage": "https://github.com/vue-youtube/vue-youtube#readme",
    "dependencies": {
        "@vue-youtube/shared": "workspace:*",
        "@vue-youtube/core": "workspace:*",
        "vue-demi": "*"
    }
}